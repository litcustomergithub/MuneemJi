t\t\t\t\t\t\t\t\t<option value=\"Tours & Travels\">Tours & Travels<\/option>\r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t<\/select>\r\n                        <\/div>\r\n                        \r\n\t\t\t\t\t\t\t\t\t\t\t<button type=\"submit\" class=\"submit-btn-partner\"><b>Submit<\/b><\/button>\r\n                    <\/form>\r\n                <\/div>\r\n                <div class=\"illustration\">\r\n                    <img decoding=\"async\" src=\"https:\/\/vyaparwebsiteimages.vypcdn.in\/marketing-images\/images\/become-a-partner.svg\" alt=\"Partnership illustration\">\r\n                <\/div>\r\n            <\/div>\r\n        <\/div>\r\n\t\t\t\t<div class=\"form-step hidden\" id=\"final-step\">\r\n\t\t\t\t\t<img decoding=\"async\" src=\"https:\/\/vyaparwebsiteimages.vypcdn.in\/marketing-images\/images\/logo-maker\/success-tick-gif.gif\" width=\"50px\" height=\"50px\">\r\n\t\t\t\t\t<h2>\r\n\t\t\t\t\t\tThank you for submitting the partner form!\r\n\t\t\t\t\t<\/h2>\r\n\t\t\t\t\t<p>\r\n\t\t\t\t\t\tOur team will review your application and get in touch with you shortly.\r\n\t\t\t\t\t<\/p>\r\n\t\t\t\t<\/div>\r\n    <\/div>\r\n<script>\r\ndocument.addEventListener('DOMContentLoaded', () => {\r\n    const initialForm = document.getElementById('initialForm');\r\n    const otpForm = document.getElementById('otpForm');\r\n    const detailsForm = document.getElementById('detailsForm');\r\n    const maskedPhoneSpan = document.getElementById('maskedPhone');\r\n    const phoneInput = window.intlTelInput(document.getElementById(\"phone\"), {\r\n        initialCountry: \"in\",\r\n        utilsScript: \"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/intl-tel-input\/17.0.8\/js\/utils.js\",\r\n        separateDialCode: true,\r\n    });\r\n\r\n    \/\/ Store user data\r\n    let userData = {};\r\n    $('#phone').on('keyup', (e) => {\r\n        let number = $('#phone').val();\r\n        number = e.target.value.replace(\/[^\\d]\/, '');\r\n        if (number.length > 10) {\r\n            number = number.substr(0, 10);\r\n        }\r\n        $('#phone').val(number);\r\n    });\r\n\r\n    $('#phone').on(\"countrychange\", function (e, countryData) {\r\n        let countryCode = $('#initialForm').find('.iti__selected-dial-code').text();\r\n        countryCode = countryCode.substring(1);\r\n        $('#country_id').val(countryCode);\r\n    });\r\n\r\n    \/\/ OTP input handling\r\n    const otpInputs = document.querySelectorAll('.otp-inputs input');\r\n    otpInputs.forEach((input, index) => {\r\n        input.addEventListener('input', (e) => {\r\n            if (e.target.value) {\r\n                if (index < otpInputs.length - 1) {\r\n                    otpInputs[index + 1].focus();\r\n                }\r\n            }\r\n        });\r\n\r\n        input.addEventListener('keydown', (e) => {\r\n            if (e.key === 'Backspace' && !e.target.value && index > 0) {\r\n                otpInputs[index - 1].focus();\r\n            }\r\n        });\r\n    });\r\n\r\n    \/\/ Initial form submission\r\n    initialForm.addEventListener('submit', async (e) => {\r\n        e.preventDefault();\r\n\r\n        userData = {\r\n            name: document.getElementById('name').value || 'Unknown',\r\n            email: document.getElementById('email').value,\r\n            phone: document.getElementById('phone').value,\r\n        };\r\n\r\n        try {\r\n            \/\/ Simulate API call to send OTP\r\n            const countryCode = $('#country_id').val();\r\n\t\t\t\t\t\tif(countryCode == '91' && (!userData.phone.match(\/^[7896]\\d{9}$\/) || userData.phone.length !== 10)) {\r\n                alert('Please enter a valid phone number');\r\n                return;\r\n            }\r\n            await sendOTP(userData.phone, countryCode);\r\n\r\n            \/\/ Show masked phone number\r\n            maskedPhoneSpan.textContent = maskPhoneNumber(userData.phone);\r\n\r\n            \/\/ Switch to OTP verification step\r\n            if (countryCode === '91') {\r\n                showStep(2);\r\n            }\r\n            else {\r\n                $('#location').attr('required', false);\r\n                $('#location-container').hide();\r\n                showStep(3);\r\n            }\r\n        } catch (error) {\r\n            alert('Error sending OTP. Please try again.');\r\n        }\r\n    });\r\n\r\n    \/\/ OTP form submission\r\n    otpForm.addEventListener('submit', async (e) => {\r\n        e.preventDefault();\r\n\r\n        const otp = Array.from(otpInputs).map(input => input.value).join('');\r\n\r\n        try {\r\n            \/\/ Simulate API call to verify OTP\r\n            const otpVerified = await verifyOTP(userData.phone, otp);\r\n            const countryCode = $('#country_id').val();\r\n\t\t\t\t\t\tif(otpVerified) {\r\n\t\t\t\t\t\t\tshowStep(3);\r\n\t\t\t\t\t\t} else {\r\n\t\t\t\t\t\t\talert('otp verification failed. Please try again');\r\n\t\t\t\t\t\t}            \r\n        } catch (error) {\r\n            alert('Invalid OTP. Please try again.');\r\n        }\r\n    });\r\n\r\n    \/\/ Details form submission\r\n    detailsForm.addEventListener('submit', async (e) => {\r\n        e.preventDefault();\r\n\r\n        const finalData = {\r\n            ...userData,\r\n            pinCode: document.getElementById('location').value,\r\n            businessType: document.getElementById('businessType').value,\r\n            businessCategory: document.getElementById('businessCategory').value,\r\n            step: 3,\r\n            current_route: 'partner-page',\r\n\t\t\tcountry_id: '91',\r\n        };\r\n\r\n        try {\r\n            \/\/ Simulate API call to submit final data\r\n            await submitPartnerData(finalData);\r\n\t\t\t\t\t\t$('#step3').hide();\r\n\t\t\t\t\t\t$('#final-step').show();\r\n        } catch (error) {\r\n            alert('Error submitting data. Please try again.');\r\n        }\r\n    });\r\n\r\n    \/\/ Helper functions\r\n    function showStep(step) {\r\n        document.querySelectorAll('.form-step').forEach(el => el.classList.add('hidden'));\r\n        document.getElementById(`step${step}`).classList.remove('hidden');\r\n    }\r\n\r\n    function maskPhoneNumber(phone) {\r\n        return `****${phone.slice(-4)}`;\r\n    }\r\n\r\n    function getCampaignDetails() {\r\n        const url = new URL(window.location.href);\r\n        const query = url.search.substring(1);\r\n        const params = query.split('&');\r\n        const campaignDetailsParams = {};\r\n        for (const item of params) {\r\n            const data = item.split('=');\r\n            campaignDetailsParams['campaign_id'] = 'partner_page_v2';\r\n            if (data[0] === 'adgroupid') {\r\n                campaignDetailsParams['ad_group_id'] = decodeURIComponent(data[1]);\r\n            }\r\n            else if (data[0] === 'network') {\r\n                campaignDetailsParams['network'] = decodeURIComponent(data[1]);\r\n            }\r\n            else if (data[0] === 'creative') {\r\n                campaignDetailsParams['creative'] = decodeURIComponent(data[1]);\r\n            }\r\n            else if (data[0] === 'keyword') {\r\n                campaignDetailsParams['keyword'] = decodeURIComponent(data[1]);\r\n            }\r\n            else if (data[0] === 'gclid') {\r\n                campaignDetailsParams['gcl_id'] = decodeURIComponent(data[1]);\r\n            }\r\n        }\r\n        return campaignDetailsParams;\r\n    }\r\n\r\n    \/\/ Simulated API calls\r\n    async function sendOTP(phone, countryCode) {\r\n        try {\r\n            \/\/ First API call to send OTP\r\n            if (countryCode === '91') {\r\n                const otpResponse = await fetch('https:\/\/vyaparapp.in\/partner\/otp', {\r\n                    method: 'POST',\r\n                    headers: {\r\n                        'Content-Type': 'application\/json',\r\n                    },\r\n                    body: JSON.stringify({\r\n                        phone: phone,\r\n                        country_code: countryCode,\r\n                    }),\r\n                });\r\n\r\n                if (countryCode === '91' && !otpResponse.ok) {\r\n                    throw new Error('Network response was not ok');\r\n                }\r\n\r\n                const otpData = await otpResponse.json();\r\n                if (otpData.status !== 'success') {\r\n                    return false;\r\n                }\r\n            }\r\n\r\n            \/\/ Get campaign details\r\n            const campaignDetails = getCampaignDetails();\r\n\r\n            \/\/ Second API call to distributor\/lead\/assignment\r\n            const leadResponse = await fetch('https:\/\/vyaparapp.in\/api\/ns\/distributor\/lead\/assignment-v2', {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application\/json',\r\n                },\r\n                body: JSON.stringify({\r\n                    name: userData.name,\r\n                    email: userData.email,\r\n                    phone: phone,\r\n                    current_route: 'partner-page',\r\n                    country_id: countryCode,\r\n                    step: 1,\r\n                    ...campaignDetails\r\n                }),\r\n            });\r\n\r\n            if (!leadResponse.ok) {\r\n                throw new Error('Network response was not ok');\r\n            }\r\n\r\n            const leadData = await leadResponse.json();\r\n\t\t\t\t\t\tif(leadData.data) {\r\n\t\t\t\t\t\t\tdocument.getElementById('location').value = leadData.data;\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t}\r\n\r\n            return leadData.status === 'success';\r\n        } catch (error) {\r\n            console.error('Error sending OTP:', error);\r\n            return false;\r\n        }\r\n    }\r\n\r\n    async function verifyOTP(phone, otp) {\r\n        try {\r\n            const response = await fetch('https:\/\/vyaparapp.in\/api\/ns\/distributor\/lead\/assignment-v2', {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application\/json',\r\n                },\r\n                body: JSON.stringify({\r\n                    email: userData.email,\r\n                    name: userData.name,\r\n                    otp: otp,\r\n                    phone: phone,\r\n                    current_route: 'partner-page',\r\n                    step: 2,\r\n                    country_id: '91',\r\n                }),\r\n            });\r\n\r\n            if (!response.ok) {\r\n                throw new Error('Network response was not ok');\r\n            }\r\n\r\n            const data = await response.json();\r\n            return data.status === 'success';\r\n        } catch (error) {\r\n            console.error('Error verifying OTP:', error);\r\n            return false;\r\n        }\r\n    }\r\n\r\n    async function submitPartnerData(data) {\r\n        try {\r\n            const response = await fetch('https:\/\/vyaparapp.in\/api\/ns\/distributor\/lead\/assignment-v2', {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application\/json',\r\n                },\r\n                body: JSON.stringify(data),\r\n            });\r\n\r\n            if (!response.ok) {\r\n                throw new Error('Network response was not ok');\r\n            }\r\n\r\n            const responseData = await response.json();\r\n\r\n\t\t\t\t\treturn responseData.status === 'success';\r\n        } catch (error) {\r\n            console.error('Error submitting partner data:', error);\r\n            return false;\r\n        }\r\n    }\r\n});       \r\n<\/script>\r\n\n<\/div>\n<\/div>\n<\/div>\n<\/div>\n\n\n\n<div class=\"wp-block-group alignfull is-vertical is-content-justification-center is-layout-flex wp-container-core-group-is-layout-25 wp-block-group-is-layout-flex\">\n<div class=\"wp-block-group format-padding\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<div style=\"height:100px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n\n\n\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-partner-statistics\">Partner Statistics<\/h2>\n\n\n\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n\n\n\n<div class=\"wp-block-columns is-layout-flex wp-container-core-columns-is-layout-2 wp-block-columns-is-layout-flex\">\n<div class=\"wp-block-column is-layout-flow wp-block-column-is-layout-flow\">\n<div class=\"wp-block-group partner-stats-container is-vertical is-content-justification-center is-layout-flex wp-container-core-group-is-layout-5 wp-block-group-is-layout-flex\">\n<figure class=\"wp-block-image size-full partner-stats-img\"><img loading=\"lazy\" decoding=\"async\" width=\"1205\" height=\"893\" src=\"https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer.webp\" alt=\"\" class=\"wp-image-52164\" style=\"object-fit:cover\" srcset=\"https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer.webp 1205w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-300x222.webp 300w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-1024x759.webp 1024w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-768x569.webp 768w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-1000x741.webp 1000w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-230x170.webp 230w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-350x259.webp 350w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-480x356.webp 480w, https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/10-M-Customer-600x445.webp 600w\" sizes=\"(max-width: 1205px) 100vw, 1205px\" \/><\/figure>\n\n\n\n<div class=\"wp-block-group partner-stats\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-10-mn\"><strong>10 Mn+<\/strong><\/h2>\n\n\n\n<h4 class=\"wp-block-heading has-text-align-center\" id=\"h-customers\">Customers<\/h4>\n<\/div><\/div>\n<\/div>\n<\/div>\n\n\n\n<div class=\"wp-block-column is-layout-flow wp-block-column-is-layout-flow\">\n<div class=\"wp-block-group is-vertical is-content-justification-center is-layout-flex wp-container-core-group-is-layout-8 wp-block-group-is-layout-flex\">\n<figure class=\"wp-block-image size-full\"><img decoding=\"async\" src=\"https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/partner-stats-2.svg\" alt=\"\" class=\"wp-image-50894\"\/><\/figure>\n\n\n\n<div class=\"wp-block-group\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<div class=\"wp-block-group partner-stats\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-27-k\"><strong>27 K+<\/strong><\/h2>\n\n\n\n<h4 class=\"wp-block-heading has-text-align-center\" id=\"h-partners\">Partners<\/h4>\n<\/div><\/div>\n<\/div><\/div>\n<\/div>\n<\/div>\n\n\n\n<div class=\"wp-block-column is-layout-flow wp-block-column-is-layout-flow\">\n<div class=\"wp-block-group is-vertical is-content-justification-center is-layout-flex wp-container-core-group-is-layout-11 wp-block-group-is-layout-flex\">\n<figure class=\"wp-block-image size-full partner-stats-img\"><img decoding=\"async\" src=\"https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/partner-stats-3.svg\" alt=\"\" class=\"wp-image-50895\"\/><\/figure>\n\n\n\n<div class=\"wp-block-group\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<div class=\"wp-block-group partner-stats\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-17-cr\"><strong>\u20b917 Cr+<\/strong><\/h2>\n\n\n\n<h4 class=\"wp-block-heading has-text-align-center\" id=\"h-commissions-paid\">Commissions Paid<\/h4>\n<\/div><\/div>\n<\/div><\/div>\n<\/div>\n<\/div>\n<\/div>\n\n\n\n<div style=\"height:100px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n<\/div><\/div>\n\n\n\n<div class=\"wp-block-group alignwide program-features-container format-padding\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-program-features\">Program Features<\/h2>\n\n\n\n<div style=\"height:40px\" aria-hidden=\"true\" class=\"wp-block-spacer\"><\/div>\n\n\n\n<div class=\"wp-block-group show-on-desktop\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<div class=\"wp-block-columns is-layout-flex wp-container-core-columns-is-layout-3 wp-block-columns-is-layout-flex\">\n<div class=\"wp-block-column is-layout-flow wp-block-column-is-layout-flow\">\n<div class=\"wp-block-group program-container is-vertical is-content-justification-center is-layout-flex wp-container-core-group-is-layout-14 wp-block-group-is-layout-flex\">\n<figure class=\"wp-block-image size-full is-resized\"><img decoding=\"async\" src=\"https:\/\/vyaparapp.in\/v\/z\/wp-content\/uploads\/2024\/12\/partner-features-1.svg\" alt=\"\" class=\"wp-image-50899\" style=\"object-fit:cover;width:70px;height:auto\"\/><\/figure>\n\n\n\n<div class=\"wp-block-group program-features\"><div class=\"wp-block-group__inner-container is-layout-constrained wp-block-group-is-layout-constrained\">\n<h2 class=\"wp-block-heading has-text-align-center\" id=\"h-earn-more\"><strong>Earn More<\/strong><\/h2>\n\n\n\n<p class=\"has-text-align-center\">Up to \u20b91 Lakh\/month with high commissions.<\/p>\n<\